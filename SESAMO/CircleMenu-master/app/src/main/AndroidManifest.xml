<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hitomi.circlemenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuTecnicos"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_lista"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".MenuSecretarias"
            android:parentActivityName=".MenuTecnicos"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Bitacora"
            android:parentActivityName=".lista"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".lista"
            android:parentActivityName=".MenuTecnicos"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>


    </application>

</manifest>